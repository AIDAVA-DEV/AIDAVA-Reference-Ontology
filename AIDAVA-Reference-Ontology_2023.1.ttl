###  https://biomedit.ch/rdf/sphn-ontology/AIDAVA/OncologyDiagnosis
<https://biomedit.ch/rdf/sphn-ontology/AIDAVA/OncologyDiagnosis> rdf:type owl:Class ;
                                                                 rdfs:subClassOf sphn:Deprecated ,
                                                                                 sphn:Diagnosis ,
                                                                                 [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty sphn:hasBodySite ;
                                                                                                          owl:someValuesFrom sphn:BodySite
                                                                                                        ]
                                                                                                        [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty sphn:hasBodySite ;
                                                                                                          owl:minCardinality "1"^^xsd:nonNegativeInteger
                                                                                                        ]
                                                                                                        [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty sphn:hasBodySite ;
                                                                                                          owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                                                        ]
                                                                                                      ) ;
                                                                                   rdf:type owl:Class
                                                                                 ] ,
                                                                                 [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty sphn:hasSubjectAge ;
                                                                                                          owl:someValuesFrom <https://loinc.org/rdf/30525-0>
                                                                                                        ]
                                                                                                        [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty sphn:hasSubjectAge ;
                                                                                                          owl:minCardinality "0"^^xsd:nonNegativeInteger
                                                                                                        ]
                                                                                                        [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty sphn:hasSubjectAge ;
                                                                                                          owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                                                        ]
                                                                                                      ) ;
                                                                                   rdf:type owl:Class
                                                                                 ] ;
                                                                 rdfs:comment "determination of the presence of an oncological disease, from expressed signs and symptoms and assessments such as biopsy, tumor marker test, imaging, or the like" ;
                                                                 rdfs:label "Oncology Diagnosis" ;
                                                                 owl:deprecated "true"^^xsd:boolean ;
                                                                 <http://www.w3.org/2004/02/skos/core#definition> "determination of the presence of an oncological disease, from expressed signs and symptoms and assessments such as biopsy, tumor marker test, imaging, or the like" ;
                                                                 <http://www.w3.org/2004/02/skos/core#note> "sphn:hasCode allowed coding system: SNOMED CT, ICD-10, ICD-O-3 Topography, ICD-O-3 Morphology, NANDA, ORPHA or other" .


